    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.12",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^4.0.2",
    "reflect-metadata": "^0.2.2",
    "vue": "^3.4.29",
    "vue-print-next": "^1.0.6",
    "vue-request": "^2.0.4",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
