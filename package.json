{
  "name": "vue-shop",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "ossrm": "ossutil rm oss://start-nest-e4sb-bucket/ -r -f",
    "osscp": "ossutil cp -r /Users/webde/Desktop/vue-shop/dist/ oss://start-nest-e4sb-bucket/",
    "oss-d": "npm run ossrm && npm run osscp",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "sta": "swagger-typescript-api -p http://localhost:9000/api-docs-json -o ./src/generated --axios --modular --extract-request-params --extract-request-body --extract-response-body",
    "dto": "pnpm add dto-data@latest"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "4.x",
    "axios": "^1.7.3",
